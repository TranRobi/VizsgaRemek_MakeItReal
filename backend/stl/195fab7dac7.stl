CREATE DATABASE Tappenz;
USE Tappenz;

-- Dolgozo tábla létrehozása
CREATE TABLE Dolgozo (
    szemelyiSz VARCHAR(20) PRIMARY KEY,
    dolgozoNeve VARCHAR(100) NOT NULL,
    telepKod INT NOT NULL,
    belepettDatum DATE NOT NULL,
    fizetes DECIMAL(10,2) NOT NULL,
    irSzam VARCHAR(10) NOT NULL,
    utcaHszEm VARCHAR(100) NOT NULL
);

-- Tappenz tábla létrehozása
CREATE TABLE Tappenz (
    id INT AUTO_INCREMENT PRIMARY KEY,
    szemelyiSz VARCHAR(20) NOT NULL,
    datum DATE NOT NULL,
    FOREIGN KEY (szemelyiSz) REFERENCES Dolgozo(szemelyiSz)
);

